<div class="col-md-9">
  <table class="table-user-index">
    <h2>Users</h2>
    <thead>
      <th>image</th>
      <th>name</th>
      <th></th>
    </thead>
    <tbody>
      <% @user.each do |user| %>
      <tr>
        <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %></td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", "/users/#{user.id}" %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>